{
    "activationEvents": [
        "onLanguage:witcherscript",
        "workspaceContains:witcher.package.json"
    ],
    "author": {
        "name": "Nicollas Ricas"
    },
    "bugs": {
        "url": "https://github.com/nicollasricas/vscode-witcherscript/issues"
    },
    "categories": [
        "Languages",
        "Extension Packs",
        "Snippets"
    ],
    "contributes": {
        "commands": [
            {
                "category": "Witcher",
                "command": "witcherscript.showWelcomePage",
                "title": "Welcome"
            },
            {
                "category": "Witcher",
                "command": "witcherscript.showSettingsPage",
                "title": "Open Settings"
            },
            {
                "category": "Witcher",
                "command": "witcherscript.cook",
                "icon": {
                    "dark": "./images/pan.svg",
                    "light": "./images/pan.svg"
                },
                "title": "Cook Mod"
            },
            {
                "category": "Witcher",
                "command": "witcherscript.launch",
                "icon": {
                    "dark": "./images/icon.png",
                    "light": "./images/icon.png"
                },
                "title": "Launch Game"
            },
            {
                "category": "Witcher",
                "command": "witcherscript.importScript",
                "title": "Import Witcher Script File"
            },
            {
                "category": "Witcher",
                "command": "witcherscript.importContent",
                "title": "Import Witcher Content File"
            },
            {
                "command": "witcherscript.compareScript",
                "title": "Compare with Original Witcher Script"
            }
        ],
        "configuration": [
            {
                "properties": {
                    "witcherscript.gamePath": {
                        "default": "",
                        "description": "The path containing the Witcher 3 files.",
                        "type": "string"
                    },
                    "witcherscript.modKitPath": {
                        "default": "",
                        "description": "The path containing the Witcher 3 Mod Kit files.",
                        "type": "string"
                    },
                    "witcherscript.modMergerPath": {
                        "default": "",
                        "description": "The path containing the Mod Merger files.",
                        "type": "string"
                    },
                    "witcherscript.uncookedBasePath": {
                        "default": "",
                        "description": "The path containing uncooked base files.",
                        "type": "string"
                    }
                },
                "title": "Witcher Script configuration"
            }
        ],
        "grammars": [
            {
                "language": "witcherscript",
                "path": "./syntaxes/witcherscript.json",
                "scopeName": "source.ws"
            }
        ],
        "jsonValidation": [
            {
                "fileMatch": "witcher.package.json",
                "url": "./validations/witcherscript.json"
            }
        ],
        "languages": [
            {
                "aliases": [
                    "Witcher Script",
                    "witcherscript"
                ],
                "configuration": "./language-configuration.json",
                "extensions": [
                    ".ws"
                ],
                "id": "witcherscript"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "witcherscript.compareScript",
                    "group": "witcherscript",
                    "when": "resourceLangId == witcherscript"
                }
            ],
            "editor/title": [
                {
                    "command": "witcherscript.cook",
                    "group": "navigation",
                    "when": "resourceLangId == witcherscript"
                },
                {
                    "command": "witcherscript.launch",
                    "group": "navigation",
                    "when": "resourceLangId == witcherscript"
                }
            ],
            "explorer/context": [
                {
                    "command": "witcherscript.importScript",
                    "group": "witcherscript"
                },
                {
                    "command": "witcherscript.importContent",
                    "group": "witcherscript"
                },
                {
                    "command": "witcherscript.compareScript",
                    "group": "witcherscript",
                    "when": "resourceExtname == .ws"
                }
            ]
        },
        "snippets": [
            {
                "language": "witcherscript",
                "path": "./snippets/witcherscript.json"
            }
        ]
    },
    "dependencies": {
        "tmp": "^0.0.33",
        "yauzl": "^2.9.1",
        "mkdirp": "^0.5.1"
    },
    "description": "Witcher Script for Visual Studio Code",
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-sass": "^3.1.0",
        "@types/node": "^9.4.7",
        "tslint": "^5.9.1",
        "typescript": "^2.7.2",
        "vscode": "^1.1.17"
    },
    "displayName": "vscode-witcherscript",
    "engines": {
        "vscode": "^1.23.0"
    },
    "extensionDependencies": [
        "nicollasricas.vscode-witcherscript-addon"
    ],
    "homepage": "https://github.com/nicollasricas/vscode-witcherscript/",
    "icon": "images/icon.png",
    "keywords": [
        "witcher"
    ],
    "main": "./out/extension",
    "name": "vscode-witcherscript",
    "publisher": "nicollasricas",
    "repository": {
        "type": "git",
        "url": "https://github.com/nicollasricas/vscode-witcherscript.git"
    },
    "runtimeDependencies": [
        {
            "description": "Witcher Script Companion App (1.0.2.0)",
            "installPath": ".wsc",
            "installTestPath": "./wsc/wsc.exe",
            "url": "https://github.com/nicollasricas/vscode-witcherscript-companion/releases/download/1.0.2.0/1.0.2.zip"
        }
    ],
    "scripts": {
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "vscode:prepublish": "npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "version": "1.0.7"
}